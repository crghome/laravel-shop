{
    "name": "crghome/laravel-shop",
    "description": "SHOP - shop in site for Laravel 9+",
    "version": "0.2.1",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Tarakanov",
            "email": "crghome@mail.ru",
            "homepage": "https://crghome.ru/",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Crghome\\Shop\\": "src/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Crghome\\Shop\\": "test/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/console": "^9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.3"
    },
    "scripts": {
		"test": [
	  		"vendor/bin/phpunit"
		]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crghome\\Shop\\Providers\\ShopServiceProvider"
            ]
        }
    }
}